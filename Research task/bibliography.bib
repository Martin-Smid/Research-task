number = {1},
pages = {238},
author = {A Haungs and  W D Apel and  F Badea and  K Bekk and  A Bercuci and  M Bertaina and  J Bliimer and  H Bozdog and  I M Brancus and  M Brüggemann and  P Buchholz and  A Chiavassa and  K Daumiller and  F Di Pierre and  P Doll and  R Engel and  J Engler and  P L Ghia and  H J Gils and  R Glasstetter and  C Grupen and  D Heck and  J R Hörandel, and  K-H Kampert and  H O Klages and  Y Kolotaev and  G Maier and  H J Mathes and  H J Mayer and  J Milke and  B Mitricat and  C Morello and  M Müller and  G Navarra and  R Obenland and  J Oehlschläger and  S Ostapchenko and  S Over and  M Petcut and  T Pierog and  S Plewnia and  H Rebel and  A Risse1 and  M Roth and  H Schieler and  O Sima and  M Stümpert and  G Toma and  G C Trinchero and  H Ulrich and  S Valchierotti and  J van Buren and  W Walkowiak and  A Weindl and  J Wochele and  J Zabierowski and  S Zagromski and  D Zimmermann},
title = {Investigating the 2nd knee: The KASCADE-Grande experiment},
journal = {Journal of Physics: Conference Series},
abstract = {Recent results from the multi-detector set-up KASCADE on measurements of cosmic rays in the energy range of the so called "first" knee (at ≈ 3 PeV) indicate a distinct knee in the energy spectra of light primary cosmic rays and an increasing dominance of heavy ones towards higher energies. This leads to the expectation of knee-like features of the heavy primaries at around 100 PeV. To investigate this energy region KASCADE has recently been extended by a factor 10 in area to the new experiment KASCADE-Grande. Main results of KASCADE as well as set-up, capabilities, and status of KASCADE-Grande are presented.}
}


